
.page{
	padding-top: 80rpx;
	height: 100vh;
	background-color: #fff;
}
.content{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin: 20rpx 40rpx;
	font-size: 28rpx;
	height: 72rpx;
	line-height: 70rpx;
}
.content .input-in{
	border: 1px solid #a5aab3;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 24rpx;
	font-size: 28rpx;
	border-top-left-radius: 6rpx;
	border-bottom-left-radius: 6rpx;
	border-right: 0;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.content .input-in .input{
	height: 70rpx;
	line-height: 70rpx;
}
.content .icons{
	font-size: 50rpx;
	border: 1px solid #a5aab3;
	padding: 0 10rpx;
	border-top-right-radius: 6rpx;
	border-bottom-right-radius: 6rpx;
}
.content .icons.l-none{
	border-left: 0;
	font-size: 40rpx;
}

